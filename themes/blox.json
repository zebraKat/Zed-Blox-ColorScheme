{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Blox",
  "author": "ZebraKat",
  "themes": [
    {
      "name": "Blox",
      "appearance": "dark",
      "style": {
        "background": "#303030",

        "editor.invisible": "#90909045",
        "editor.foreground": "#e1e1e1",
        "editor.background": "#303030",
        "editor.active_line.background": "#2B2B2B",
        "editor.gutter.background": "#2B2B2B",
        "editor.subheader.background": "#2E2E2E",
        "editor.document_highlight.read_background": "#202020",
        "editor.document_highlight.write_background": "#252525",
        "editor.highlighted_line.background": "#1F1F1F",
        "editor.line_number": "#a0a0a0",
        "editor.active_line_number": "#fEfEfE",

        "predictive": "#454545",
        "predictive.background": "#2B2B2B",
        "predictive.border": "#222222",

        "element.background": "#454545",
        "element.selected": "#555555",
        "element.active": "#757575",
        "element.hover": "#353535",
        "element.disabled": "#1F1F1F",

        "text": "#e1e1e1",
        "text.accent": "#e8e8e8",
        "text.muted": "#bebebe",
        "text.disabled": "#aFaFaF",
        "text.placeholder": "#eBeBeB",

        "surface.background": "#2E2E2E",
        "elevated_surface.background": "#353535",

        "status_bar.background": "#303030",

        "pane.focused_border": "#272727",
        "panel.background": "#242424",
        "panel.focused_border": "#272727",

        "info": "#FFFFFF",
        "info.background": "#242424",
        "info.border": "#303030",

        "scrollbar.thumb.border": "#222222",
        "scrollbar.thumb.hover_background": "#464646",
        "scrollbar.track.background": "#292929",
        "scrollbar.track.border": "222222",
        "scrollbar_thumb.background": "#383838",

        "toolbar.background": "#353535",

        "tab_bar.background": "#2E2E2E",
        "tab.active_background": "#252525",
        "tab.inactive_background": "#2E2E2E",

        "title_bar.background": "#242424",

        "border": "#222222",
        "border.disabled": "181818",
        "border.focused": "#282828",
        "border.selected": "#313131",
        "border.transparent": "#00000000",
        "border.variant": "#1F1F1F",

        "drop_target.background": "#222222",

        "hint": "#FFFFFF",
        "hint.background": "#242424",
        "hint.border": "#222222",

        "warning": "#867a61",
        "warning.border": "#867a61",
        "warning.background": "#242424",

        "error.border": "#846464",
        "error": "#846464",
        "error.background": "#242424",

        "deleted": "#704347",
        "deleted.background": "#242424",
        "deleted.border": "#222222",

        "created": "#508946",
        "created.background": "#242424",
        "created.border": "#222222",

        "ignored": "#4A4E87",
        "ignored.background": "#242424",
        "ignored.border": "#222222",

        "hidden": "#B2B2B2",
        "hidden.border": "#222222",
        "hidden.background": "#242424",

        "modified": "#0F8475",
        "modified.background": "#242424",
        "modified.border": "#222222",

        "unreachable": "#C67675",
        "unreachable.background": "#242424",
        "unreachable.border": "#222222",

        "conflict": "#657282",
        "conflict.background": "#242424",
        "conflict.border": "#222222",

        "ghost_element.background": "#212121",
        "ghost_element.active": "#3E3E3E",
        "ghost_element.hover": "#2a2a2a",
        "ghost_element.selected": "#2a2a2a",
        "ghost_element.disabled": "#1E1E1E",

        "icon": "#FFFFFF",
        "icon.accent": "#657282",
        "icon.muted": "#EFEFEF",
        "icon.disabled": "#efefef",
        "icon.placeholder": "#EFEFEF",

        "terminal.background": "#2E2E2E",
        "terminal.foreground": "#e1e1e1",
        "terminal.bright_foreground": "#e5e5e5",
        "terminal.dim_foreground": "#dfdFdF",

        "terminal.ansi.black": "#0F0F0F",
        "terminal.ansi.white": "#e1e1e1",
        "terminal.ansi.blue": "#587ECE",
        "terminal.ansi.red": "#CC5757",
        "terminal.ansi.green": "#6BCC57",
        "terminal.ansi.cyan": "#57CC82",
        "terminal.ansi.yellow": "#CCB657",
        "terminal.ansi.magenta": "#A357CC",

        "players": [
          {
            "cursor": "#FFFFFF75",
            "selection": "#191919"
          }
        ],
        "syntax": {
          "attribute": { "color": "#c7c7c7" },
          "boolean": { "color": "#a5a5a5" },
          "comment": { "color": "#565656" },
          "comment.documentation": { "font_style": "italic" },
          "constant": { "color": "#c7c7c7" },
          "constructor": { "color": "#b8b8b8" },
          "function": { "color": "#c5c5c5" },
          "function.builtin": { "color": "#797979" },
          "function.method": { "color": "#626262" },
          "keyword": { "color": "#c7c7c7" },
          "operator": { "color": "#5d5d5d" },
          "parameter": { "color": "#888888" },
          "property": { "color": "#797979" },
          "punctuation": { "color": "#5D5D5D" },
          "punctuation.bracket": { "color": "#ababab" },
          "string": { "color": "#d0d0d0" },
          "type": { "color": "#848484" },
          "variable": { "color": "#adadad" },
          "variable.special": { "color": "#888888" }
        }
      }
    }
  ]
}
